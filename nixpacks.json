{
    "phases": {
        "setup": {
            "cmds": [
                "apt-get update && apt-get install -y python3 python3-pip"
            ]
        },
        "build": {
            "cmds": [
                "pip3 install -r requirements.txt"
            ]
        },
        "start": {
            "cmd": "bash start.sh"
        }
    }
}
